{
  "rules": {
    "camelcase": [0, {"properties": "never"}],
    "no-console": 0,
    "max-len": ["error", 120, 2, { "ignoreComments": true }]
  },
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "generators": true,
      "experimentalObjectRestSpread": true
    }
  }
}
