{
  "rules": {
    "camelcase": [0, {"properties": "never"}],
    "max-len": ["error", 120, 2, { "ignoreComments": true }]
  },
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  }
}
